#include <iostream>
#include <string>
using namespace std;

int main() {
	string N; 
	string n;
	int F;
	int share;
	cin >> N;
	cin >> F;
	N[N.size() - 2] = '0';
	N[N.size() - 1] = '0';
	share = stoi(N) / F;
	if (stoi(N) % F == 0) {
		cout << N[N.size() - 2] << N[N.size() - 1] << endl;
	}
	else {
		n = to_string(share * F + F);
		cout << n[n.size() - 2] << n[n.size() - 1] << endl;
	}
}
//https://www.acmicpc.net/problem/1075
